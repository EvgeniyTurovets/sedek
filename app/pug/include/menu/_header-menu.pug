.wrapper
    nav.header-menu
        .container
            .row.justify-content-between
                .col-6.col-sm-8.col-lg-9
                    .row.justify-content-between
                        each val, key in headerMenu
                            .col-12.col-md-4.header-menu_col
                                each subVal, subKey in val
                                    ul.header-menu_list
                                        if(subVal.name === '% Акции')
                                            li.header-menu_item
                                                a(class='header-menu_link promo-menu-link' href=subVal.link style='color:red')=subVal.name
                                        else
                                            li.header-menu_item
                                                a(class='header-menu_link' href=subVal.link)=subVal.name

                                                if(subVal.subLevel)
                                                    ul.header-submenu_list.d-none.d-md-block
